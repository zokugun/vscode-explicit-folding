{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"outDir": "out",
		"lib": [
			"es6",
			"es2017"
		],
		"sourceMap": false,
		"rootDir": "src",
		"strictNullChecks": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"strict": true,
		"alwaysStrict": true
	},
	"exclude": [
		"node_modules",
		".vscode-test"
	]
}
